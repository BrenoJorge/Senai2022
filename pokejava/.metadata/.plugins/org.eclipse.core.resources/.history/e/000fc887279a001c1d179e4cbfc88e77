package bens;

public class Snippet {
	public int id;
		public String nome;
		public int idade;
	    public String telefone;
	    
	    Contato(){}
	    
	    Contato(int id, String nome, int idade, String telefone){
	    	this.id = id;
	    	this.nome = nome;
	    	this.idade = idade;
	    	this.telefone = telefone;
	    }
	    
	    public String contatotab() {
	    	return id + "\t" + nome + "\t" + idade + "\t" + telefone;
	    }
	}
}

