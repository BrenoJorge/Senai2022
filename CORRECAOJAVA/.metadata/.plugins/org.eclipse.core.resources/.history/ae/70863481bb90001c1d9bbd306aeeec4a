package correcao;
import java.util.Scanner;
public class Impossivel {

	public static void main(String[] args) {
	    
		leVetor();
		int[] idade = new int[5];
		
	}
	public static int[] leVetor() {
		int[] idade = new int[5];
		Scanner leia = new Scanner(System.in);
		for(int i = 0; i < 5; i++);
		System.out.println("Digite as idades: ");
		idade[i] = leia.nextInt();
		return idade;
	}
}
