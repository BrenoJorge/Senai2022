package correcao;
import java.util.Random;
public class Correcao {
		public static void main(String[] args) {
		
			imprimir();
			
			
		}
		
		public static void imprimir() {
			
			String nome[] = new String[4];
			nome[0] = "felipe";
			nome[1] = "joa";
			nome[2] = "vitor";
			nome[3] = "julia";
			
			
			
			
			String sobrenome[] = new String[4];
			sobrenome[0] = "silva";
			sobrenome[1] = "lima";
			sobrenome[2] = "roberto";
			sobrenome[3] = "carlos";
			
			
			
			for(int i =0; i < 4; i++) {
			Random ale = new Random();
			int nome_ale = ale.nextInt(nome.length);
			int sobre_ale = ale.nextInt(4);
			
			System.out.print(nome[nome_ale] + " ");
			System.out.println(sobrenome[sobre_ale]);
			 }
		}
}
