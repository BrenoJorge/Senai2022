package correcao;

public class CartaoCreditoDebito {

	public static void main(String[] args) {
		System.out.println();
		System.out.println();
		System.out.println(simples());

	}
	public static int simples() {
		int x = 0;
		for(int i = 0;i < 10;i++) {
			x+= i;
			System.out.println(x);
		}
		return x;
	}
}
